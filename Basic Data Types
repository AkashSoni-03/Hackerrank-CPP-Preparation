#include <iostream>
#include <cstdio>
using namespace std;
int main() {
    // Complete the code.
    int intNumber;
    long longNumber;
    char character;
    float floatNumber;
    double doubleNumber;
    scanf("%d %ld %c %f %lf",&intNumber,&longNumber,&character,&floatNumber,&doubleNumber);
    printf("%d\n",intNumber);
    printf("%ld\n",longNumber);
    printf("%c\n",character);
    printf("%f\n",floatNumber);
    printf("%lf\n",doubleNumber);
    return 0;
}
